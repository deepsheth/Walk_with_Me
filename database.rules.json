{
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    ".indexOn": [
      "users",
      "chats",
      "posted_walks",
      "current_walks",
      "past_walks",
      "walk_history"
    ],
    "$user_id": {
      ".indexOn": [
        "name",
        "profile_picture",
        "school",
        "email",
        "current_walk",
        "icon_style"
      ]
    }
  }
}
